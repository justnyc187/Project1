<body>

  <h1>WOAT! Login</h1>

    <div class="login-div">
        <form class="login-p" action="/login" method="POST">
              
          <p>
            <input class="login-input" type="email" name="email" placeholder="Email"  required>
          </p>
          <p>
            <input class="login-input" type="password" name="password" placeholder="Password"  required minlength="8">
